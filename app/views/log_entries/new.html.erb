<div class="page">
<h1>New Log Entry</h1>


<%=form_tag({ :controller => 'log_entries', :action => 'create' },
{ :method => :post}) do %>
  
<%= label_tag :タイトル %>
<%= text_field_tag :name %>
<%= label_tag :内容 %>
<%= text_area_tag :content %>
<%= submit_tag "追加" %>
  
<% end %>


<%= render 'form', log_entry: @log_entry %>

<%= link_to 'Back', log_entries_path %>


</div>

